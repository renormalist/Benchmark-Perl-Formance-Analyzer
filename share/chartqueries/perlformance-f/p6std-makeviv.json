{
   "charttitle" : "P6STD - parsing Perl6 in Perl5",
   "chartlines" : [
       {
           "title" : "P6STD.makeviv/threads",
           "query" : {
               "select" : [ "NAME", "VALUE", "perlconfig_version" ],
               "where" : [
                   [  "=", "NAME", "perlformance.perl5.P6STD.makeviv(F)" ],
                   [  ">", "VALUE", 1 ],
                   [  "=", "sysinfo_cpu_name", "AMD Opteron(tm) Processor 4180" ],
                   [ "like", "bootstrap_perl_symbolic_name", "%-thread%" ]
               ],
               "order_by" : [ "VALUE_ID" ]
           }
       },
       {
           "title" : "P6STD.makeviv/nothreads",
           "query" : {
               "select" : [ "NAME", "VALUE", "perlconfig_version" ],
               "where" : [
                   [  "=", "NAME", "perlformance.perl5.P6STD.makeviv(F)" ],
                   [  ">", "VALUE", 1 ],
                   [  "=", "sysinfo_cpu_name", "AMD Opteron(tm) Processor 4180" ],
                   [ "like", "bootstrap_perl_symbolic_name", "%-nothread%" ]
               ],
               "order_by" : [ "VALUE_ID" ]
           }
       }
   ]
}
