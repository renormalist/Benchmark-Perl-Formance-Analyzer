{
   "charttitle" : "Mem - memory stress",
   "chartlines" : [
       {
           "title" : "mem.allocate/threads",
           "query" : {
               "select" : [ "NAME", "VALUE", "perlconfig_version" ],
               "where" : [
                   [  "=", "NAME", "perlformance.perl5.Mem.allocate(F)" ],
                   [  "=", "sysinfo_cpu_name", "AMD Opteron(tm) Processor 4180" ],
                   [ "like", "bootstrap_perl_symbolic_name", "%-thread%" ]
               ],
               "order_by" : [ "VALUE_ID" ]
           }
       },
       {
           "title" : "mem.allocate/nothreads",
           "query" : {
               "select" : [ "NAME", "VALUE", "perlconfig_version" ],
               "where" : [
                   [  "=", "NAME", "perlformance.perl5.Mem.allocate(F)" ],
                   [  "=", "sysinfo_cpu_name", "AMD Opteron(tm) Processor 4180" ],
                   [ "like", "bootstrap_perl_symbolic_name", "%-nothread%" ]
               ],
               "order_by" : [ "VALUE_ID" ]
           }
       },
       {
           "title" : "mem.copy/threads",
           "query" : {
               "select" : [ "NAME", "VALUE", "perlconfig_version" ],
