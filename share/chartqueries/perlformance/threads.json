{
   "charttitle" : "Threads - thread handling",
   "chartlines" : [
       {
           "title" : "threadstorm/threads",
           "query" : {
               "select" : [ "NAME", "VALUE", "perlconfig_version" ],
               "where" : [
                   [  "=", "NAME", "perlformance.perl5.Threads.threadstorm" ],
                   [ "like", "bootstrap_perl_symbolic_name", "%-thread%" ]
               ],
               "order_by" : [ "VALUE_ID" ]
           }
       },
       {
           "title" : "threadstorm/nothreads",
           "query" : {
               "select" : [ "NAME", "VALUE", "perlconfig_version" ],
               "where" : [
                   [  "=", "NAME", "perlformance.perl5.Threads.threadstorm" ],
                   [ "like", "bootstrap_perl_symbolic_name", "%-nothread%" ]
               ],
               "order_by" : [ "VALUE_ID" ]
           }
       }
   ]
}
